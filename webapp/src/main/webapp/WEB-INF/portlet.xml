<?xml version="1.0" encoding="UTF-8"?>
<portlet-app version="1.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

  <portlet>
    <description xml:lang="EN">Meeting</description>
    <portlet-name>MeetingPortlet</portlet-name>
    <display-name xml:lang="EN">Meeting Portlet</display-name>
    <portlet-class>org.exoplatform.webui.application.portlet.PortletApplicationController</portlet-class>
    <init-param>
      <name>webui.configuration</name>
      <value>/WEB-INF/conf/portlet/meeting/webui/configuration.xml</value>
    </init-param>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <resource-bundle>locale.portlet.MeetingPortlet</resource-bundle>
    <portlet-info>
      <title>Meeting Portlet</title>
      <short-title>Meeting Portlet</short-title>
      <keywords>MeetingPortlet</keywords>
    </portlet-info>
  </portlet>

</portlet-app>
