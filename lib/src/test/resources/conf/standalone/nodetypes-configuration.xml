<nodeTypes xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0">

  <nodeType name="teamH:timerange" isMixin="false"
    hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      <!-- List user vote for this time range -->
      <propertyDefinition name="mt:user"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Begin of time range -->
      <propertyDefinition name="mt:begin"
        requiredType="Date" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- End of time range -->
      <propertyDefinition name="mt:end" requiredType="Date"
        autoCreated="false" mandatory="false" onParentVersion="COPY"
        protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Is final time range -->
      <propertyDefinition name="mt:final"
        requiredType="Boolean" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
    </propertyDefinitions>
  </nodeType>

  <nodeType name="teamH:meetingroom" isMixin="false"
    hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      <!-- Title of a meeting room -->
      <propertyDefinition name="mt:title"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
    </propertyDefinitions>
  </nodeType>

  <nodeType name="teamH:meetingcomment" isMixin="false"
    hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      <!-- Owner of meeting comment -->
      <propertyDefinition name="mt:owner"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Body of a meeting comment -->
      <propertyDefinition name="mt:comment"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
    </propertyDefinitions>
  </nodeType>

  <nodeType name="teamH:meeting" isMixin="false"
    hasOrderableChildNodes="false" primaryItemName="">
    <supertypes>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      <!-- Title of a meeting -->
      <propertyDefinition name="mt:title"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Description of a meeting -->
      <propertyDefinition name="mt:description"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Location of a meeting -->
      <propertyDefinition name="mt:location"
        requiredType="teamH:meetingroom" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Comment of a meeting -->
      <propertyDefinition name="mt:comment"
        requiredType="teamH:meetingcomment" autoCreated="false"
        mandatory="false" onParentVersion="COPY" protected="false"
        multiple="true">
        <valueConstraints />
      </propertyDefinition>
      <!-- Username of meeting's creator -->
      <propertyDefinition name="mt:creator"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Participants of a meeting which will keep the username of invited 
        people -->
      <propertyDefinition name="mt:participants"
        requiredType="String" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Date of a meeting -->
      <propertyDefinition name="mt:date"
        requiredType="Date" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Last update of a meeting -->
      <propertyDefinition name="mt:update"
        requiredType="Date" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Time range provide for a meeting -->
      <propertyDefinition name="mt:timeslot"
        requiredType="teamH:timerange" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="true">
        <valueConstraints />
      </propertyDefinition>
      <!-- Allow the owner decide the selection is one or many -->
      <propertyDefinition name="mt:selectOption"
        requiredType="Boolean" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Limit number of participants -->
      <propertyDefinition name="mt:numberOfParticipants"
        requiredType="Long" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
      <!-- Hidden the answer - Only Owner can see the answers -->
      <propertyDefinition name="mt:hiddenAnswers"
        requiredType="Boolean" autoCreated="false" mandatory="false"
        onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints />
      </propertyDefinition>
    </propertyDefinitions>
  </nodeType>



</nodeTypes>
